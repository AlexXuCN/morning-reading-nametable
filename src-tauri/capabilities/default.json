{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "clipboard-manager:default",
    "clipboard-manager:allow-write-text",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "store:allow-has",
    "store:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "core:window:allow-close"
  ]
}
